<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchorPane" fx:id="anchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphic.MainRenderFXMLControl">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@fireball.png" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <Label alignment="CENTER" layoutX="59.0" layoutY="34.0" prefHeight="44.0" prefWidth="483.0" text="The Defender" wrapText="true">
         <font>
            <Font name="Cantarell Extra Bold" size="75.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="178.0" layoutY="130.0" text="Of the universe" wrapText="true">
         <font>
            <Font name="Cantarell Extra Bold" size="34.0" />
         </font>
      </Label>
      <Button fx:id="startButton" layoutX="253.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#startButtonHandler" prefHeight="44.0" prefWidth="96.0" text="Start" textOverrun="WORD_ELLIPSIS">
         <font>
            <Font name="Noto Mono" size="19.0" />
         </font>
      </Button>
      <Button fx:id="howToPlayButton" layoutX="257.0" layoutY="257.0" mnemonicParsing="false" onMouseClicked="#howToPlayButtonHandler" prefHeight="25.0" prefWidth="89.0" text="How to play" textOverrun="CLIP">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
